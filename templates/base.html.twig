<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="application-name" content="optigas"/>
        <meta name="author" content="abwopda@yahoo.com">
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
        {% block meta %}{% endblock %}
        <title>{% block title %}{{ app_config.app_name }}{% endblock %}</title>
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('images/eneo.png') }}" />
        <style>{{ app_config.app_css }}</style>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
            <link href="{{ asset('css/bootstrap.css') }}" rel="stylesheet">
            <link href="{{ asset('css/layout.css') }}" rel="stylesheet">
            <link href="{{ asset('css/main.css') }}" rel="stylesheet">
            <link href="{{ asset('css/select2.css') }}" rel="stylesheet">
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
            <script src="{{ asset('js/jquery.min.js') }}"></script>
            <script src="{{ asset('js/bootstrap.js') }}"></script>
            <script src="{{ asset('js/jquery.cookie.js') }}"></script>
            <script src="{{ asset('js/select2.min.js') }}"></script>
            <script>
                (function($) {
                    $('select').select2();
                    /*$('select').select2({
                        theme: "bootstrap"
                    });*/
                })(jQuery);
            </script>
        {% endblock %}
    </head>
    {% block container %}
    <body>
        {% block body %}{% endblock %}
    </body>
    {% endblock container %}
</html>
